{
  "name": "my-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "adrien2p <adrien.deperetti@gmail.com>",
  "homepage": "https://github.com/adrien2p",
  "license": "MIT",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.17.3"
  },
  "scripts": {
    "seed": "medusa seed -f ./data/seed.json",
    "typeorm": "typeorm-ts-node-commonjs -d src/ormconfig.js",
    "medusa": "medusa",
    "migration:generate": "yarn run build && yarn run typeorm migration:generate && yarn run build",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "yarn run medusa migrations run",
    "db:reset": "yarn run typeorm query \"DROP SCHEMA public CASCADE; CREATE SCHEMA public; GRANT USAGE ON SCHEMA public to PUBLIC; GRANT CREATE ON SCHEMA public to PUBLIC; COMMENT ON SCHEMA public IS 'standard public schema';\" && yarn run migration:run",
    "db:user": "medusa user -e admin@medusa-test.com -p supersecret",
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "build:admin": "rm -rf build && medusa-admin build",
    "dev": "medusa develop",
    "start": "medusa start"
  },
  "dependencies": {
    "@medusajs/admin": "^2.0.0",
    "@medusajs/cache-redis": "^1.8.0",
    "@medusajs/event-bus-redis": "^1.8.0",
    "@medusajs/inventory": "^1.8.0",
    "@medusajs/medusa": "^1.8.0",
    "@medusajs/stock-location": "^1.8.0",
    "awilix": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^12.0.4",
    "express": "^4.17.1",
    "glob": "^9.3.4",
    "medusa-core-utils": "^1.2.0",
    "medusa-fulfillment-manual": "^1.1.36",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.22",
    "pg": "^8.10.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-typescript": "^7.14.5",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-preset-medusa-package": "^1.1.19",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
